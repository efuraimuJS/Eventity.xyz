{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "events_orders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "inversedBy": "ordered_events"
    },
    "total_price": {
      "type": "decimal",
      "required": true,
      "default": 0,
      "min": 0
    },
    "order_quantity": {
      "default": 1,
      "min": 1,
      "required": true,
      "type": "integer"
    },
    "order_uid": {
      "type": "uid"
    },
    "order_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "users_order"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "PENDING",
        "PAID"
      ],
      "required": true,
      "default": "PENDING"
    }
  }
}
